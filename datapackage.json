        "$schema": "http://json-schema.org/schema#",
        "properties": {
          "_id": {
            "type": "string"
          },
          "actions": {
            "description": "A list of actions parliament has taken on this bill, in chronological order.  An action must be one of the following predefined types.\n",
            "items": {
              "oneOf": [
                {
                  "description": "Submission (first reading) of a bill.",
                  "properties": {
                    "action": {
                      "enum": [
                        "submission"
                      ],
                      "type": "string"
                    },
                    "committees_referred_to": {
                      "description": "The committees the bill was referred to.",
                      "type": [
                        "string",
                        "null"
                      ]
                    },
                    "plenary_sitting_id": {
                      "description": "The plenary sitting's ID.",
                      "type": "string"
                    },
                    "sponsors": {
                      "description": "The bill's sponsors.",
                      "type": "string"
                    },
                    "title": {
                      "description": "The bill's title upon submission.",
                      "type": "string"
                    }
                  },
                  "required": [
                    "action",
                    "plenary_sitting_id",
                    "sponsors",
                    "title"
                  ],
                  "type": "object"
                }
              ],
              "type": "object"
            },
            "type": "array"
          },
          "explanatory_note": {
            "description": "An accompanying explanatory note as published in the Government Gazette.\n",
            "properties": {
              "authors": {
                "description": "The author(s) of the note.",
                "items": {
                  "properties": {
                    "name": {
                      "description": "The name of the author, in Greek, in the following order: last name, middle name, first name.\n",
                      "type": "string"
                    },
                    "note": {
                      "description": "What's written under the author's name, usually the capacity in which they're proposing a bill.\n",
                      "type": [
                        "string",
                        "null"
                      ]
                    }
                  },
                  "required": [
                    "name"
                  ],
                  "type": "object"
                },
                "type": "array"
              },
              "in": {
                "description": "Where the note can be found in the Gazette.",
                "properties": {
                  "issue": {
                    "description": "The issue of the Gazette.",
                    "type": "integer"
                  },
                  "pages": {
                    "description": "Page or page range where the note has been printed. If a page range is used, separate pages with an en dash, e.g. '546\u20137'.\n",
                    "pattern": "^\\d+(\u2013\\d+)?$",
                    "type": [
                      "string",
                      "number"
                    ]
                  }
                },
                "required": [
                  "issue",
                  "pages"
                ],
                "type": "object"
              },
              "text": {
                "description": "The note itself, in Markdown.",
                "type": "string"
              }
            },
            "required": [
              "authors",
              "in",
              "text"
            ],
            "type": [
              "object",
              "null"
            ]
          },
          "identifier": {
            "description": "The bill's number assigned to it by parliament.",
            "type": "string"
          },
          "law": {
            "description": "The law number, if the bill has been enacted.",
            "type": [
              "string",
              "null"
            ]
          },
          "sources": {
            "description": "A list of references used by editors in prose (e.g. in the `summary` field), or anywhere you might wanna direct readers.\n",
            "items": {
              "properties": {
                "id": {
                  "description": "A keyword for this source, to be used as an anchor. Not required if not a reference.\n",
                  "type": [
                    "string",
                    "null"
                  ]
                },
                "text": {
                  "description": "The source, in Markdown.",
                  "format": "markdown",
                  "type": "string"
                }
              },
              "required": [
                "text"
              ],
              "type": "object"
            },
            "type": "array"
          },
          "status": {
            "description": "The status of the bill.",
            "enum": [
              "pass",
              "fail",
              "withdrawn",
              "pending"
            ],
            "type": [
              "string",
              "null"
            ]
          },
          "summary": {
            "description": "A summary of this bill authored by an editor, in Markdown.",
            "format": "markdown",
            "type": [
              "string",
              "null"
            ]
          },
          "title": {
            "description": "The title of this bill. This should be the title used in parliament, which may be different from the title of the law, if the bill has been enacted.\n",
            "type": "string"
          },
          "votes": {
            "items": {
              "description": "A vote event corresponding to an action.",
              "properties": {
                "breakdown": {
                  "description": "Individual MP votes; inferred.",
                  "items": {
                    "properties": {
                      "name": {
                        "description": "The name of the MP, in Greek, in the following order: last name, middle name, first name.\n",
                        "type": "string"
                      },
                      "option": {
                        "description": "Whether the MP has voted for or against, or abstained.\n",
                        "enum": [
                          "yes",
                          "no",
                          "abstain"
                        ],
                        "type": "string"
                      }
                    },
                    "required": [
                      "name",
                      "option"
                    ],
                    "type": "object"
                  },
                  "type": "array"
                },
                "counts": {
                  "additionalItems": false,
                  "description": "Official counts. Options must be in the following order: yes, no, abstain. This is to ensure that there is only one of each option.\n",
                  "items": [
                    {
                      "properties": {
                        "count": {
                          "description": "The vote tally for this `option`.",
                          "type": "integer"
                        },
                        "option": {
                          "description": "Whether this option is for MPs who've voted for or against, or abstained.\n",
                          "enum": [
                            "yes"
                          ],
                          "type": "string"
                        }
                      },
                      "required": [
                        "count",
                        "option"
                      ],
                      "type": "object"
                    },
                    {
                      "properties": {
                        "count": {
                          "description": "The vote tally for this `option`.",
                          "type": "integer"
                        },
                        "option": {
                          "description": "Whether this option is for MPs who've voted for or against, or abstained.\n",
                          "enum": [
                            "no"
                          ],
                          "type": "string"
                        }
                      },
                      "required": [
                        "count",
                        "option"
                      ],
                      "type": "object"
                    },
                    {
                      "properties": {
                        "count": {
                          "description": "The vote tally for this `option`.",
                          "type": "integer"
                        },
                        "option": {
                          "description": "Whether this option is for MPs who've voted for or against, or abstained.\n",
                          "enum": [
                            "abstain"
                          ],
                          "type": "string"
                        }
                      },
                      "required": [
                        "count",
                        "option"
                      ],
                      "type": "object"
                    }
                  ],
                  "minItems": 3,
                  "type": "array"
                },
                "remarks": {
                  "description": "Editor remarks.",
                  "format": "markdown",
                  "type": "string"
                },
                "result": {
                  "description": "The outcome of the vote.",
                  "enum": [
                    "pass",
                    "fail"
                  ],
                  "type": "string"
                }
              },
              "required": [
                "counts",
                "result"
              ],
              "type": "object"
            },
            "type": "array"
          }
        },
        "required": [
          "_id",
          "identifier",
          "title"
        ],
        "type": "object"
