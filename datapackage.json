                "not": {
                  "enum": [
                    true
                  ]
                },
                "type": "array"
              }
            },
            "required": [
              "cap1",
              "cap2",
              "cap4"
            ],
            "type": "object"
          },
          "attendees": {
            "description": "A list of MPs who were present at the meeting.",
            "items": {
              "properties": {
                "mp_id": {
                  "description": "The `id` of the attending MP.",
                  "type": "string"
                }
              },
              "type": "object"
            },
            "not": {
              "enum": [
                true
              ]
            },
            "type": "array"
          },
          "date": {
            "description": "The date the plenary was held on, in the ISO format (YYYY-MM-DD).\n",
            "regex": "^[0-9]{4}(-[0-9]{2}){0,2}$",
            "type": "string"
          },
          "links": {
            "description": "Links to relevant documents.  Either the source of the agenda or the transcript.\n",
            "items": {
              "properties": {
                "type": {
                  "description": "The type of resource.",
                  "enum": [
                    "agenda",
                    "transcript"
                  ],
                  "type": "string"
                },
                "url": {
                  "description": "The link.",
                  "format": "uri",
                  "type": "string"
                }
              },
              "required": [
                "type",
                "url"
              ],
              "type": "object"
            },
            "type": "array"
          },
          "parliamentary_period_id": {
            "description": "The parliamentary period, as published on the agenda, e.g. '\u0399'.  The parliamentary period is the period between elections.\n",
            "type": "string"
          },
          "session": {
